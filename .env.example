# Site Configuration
# サイトのURL（本番環境のドメイン）
NEXT_PUBLIC_SITE_URL=https://media-work.jp

# Supabase Configuration
# SupabaseプロジェクトのURL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
# Supabase匿名キー（公開可能）
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
# Supabaseサービスロールキー（サーバーサイドのみ、公開しないこと）
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
# SupabaseプロジェクトのリファレンスID（URLの一部、例: qazyoxligvjsasfqettg）
SUPABASE_PROJECT_REF=your_supabase_project_ref
# Supabase Management API用アクセストークン（マイグレーションスクリプト用）
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# Slack Configuration
# Slack Webhook URL（Incoming Webhooks用）
SLACK_WEBHOOK_URL=your_slack_webhook_url
